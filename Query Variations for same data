--Retrieve the orders that were only placed in 2017. Used in the superstore_dataset table
Query Variation #1:
SELECT
	OrderID,
    CustomerName,
    OrderDate,
    ShipDate
FROM superstore_dataset
HAVING YEAR(OrderDate) ='2017'
ORDER BY OrderDate ASC

Query Variation #2:
SELECT
	OrderID,
    CustomerName,
    OrderDate,
    ShipDate
FROM superstore_dataset
WHERE OrderDate IN (
	SELECT
		OrderDate
	FROM superstore_dataset
    WHERE OrderDate BETWEEN '2017-01-01' AND '2017-12-31'
	)
ORDER BY OrderDate ASC

Query Variation #3:

SELECT
	OrderID,
    CustomerName,
    OrderDate,
    ShipDate
FROM superstore_dataset
WHERE OrderDate BETWEEN '2017-01-01' AND '2017-12-31'
ORDER BY OrderDate ASC



--Find orders that were shipped on weekdays (Monday to Friday). Used in the superstore_dataset table
Query Variation #1:
SELECT
	OrderID,
    ShipDate,
    DAYNAME(ShipDate)
FROM superstore_dataset
WHERE DAYNAME(ShipDate) <> 'Saturday' AND DAYNAME(ShipDate) <>'Sunday'
ORDER BY ShipDate ASC

Query Variation #2:
SELECT
	OrderID,
    ShipDate,
    DAYNAME(ShipDate)
FROM superstore_dataset
WHERE DAYNAME(ShipDate) NOT BETWEEN 'Saturday' AND 'Sunday'
ORDER BY ShipDate ASC




--Calculate the total sales for each month.
Query Variation #1:
SELECT
	MONTHNAME(OrderDate),
    SUM(Sales)
FROM superstore_dataset
GROUP BY MONTHNAME(OrderDate)
ORDER BY SUM(Sales) ASC

		
Query Variation #2:
SELECT
	MONTH(OrderDate),
    SUM(Sales)
FROM superstore_dataset
GROUP BY MONTH(OrderDate)
ORDER BY 1

